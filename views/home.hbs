<div id="map-outer"><div id="map-canvas"/></div></div>
<div class="banner-wrapper"><p id="banner">Wheaton College CS Alumni <img id="logo" src="/img/logo.png"></p></div>

<div class="list">
	{{#each people}}
		<div class="item">
			<img class="profile-picture" id={{id}} src=''/>
			<div class="profile-info">
				<div class="fName">{{firstName}} {{lastName}}</div>
				<div class="position"> {{positions.values.0.company.name}} </div>
				<div class="location"> {{location.name}} </div>
			</div>
		</div>
	{{/each}}
</div>
